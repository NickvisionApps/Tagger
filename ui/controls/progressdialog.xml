<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <object class="AdwWindow" id="adw_progDialog">
        <property name="default-width">400</property>
        <property name="default-height">60</property>
        <property name="modal">true</property>
        <property name="resizable">false</property>
        <property name="deletable">false</property>
        <property name="destroy-with-parent">false</property>
        <child>
            <object class="GtkBox">
                <property name="margin-start">10</property>
                <property name="margin-top">10</property>
                <property name="margin-end">10</property>
                <property name="margin-bottom">10</property>
                <property name="orientation">vertical</property>
                <property name="spacing">10</property>
                <child>
                    <object class="GtkLabel" id="gtk_lblDescription">
                        <property name="halign">start</property>
                    </object>
                </child>
                <child>
                    <object class="GtkProgressBar" id="gtk_progBar"></object>
                </child>
            </object>
        </child>
    </object>
</interface>
