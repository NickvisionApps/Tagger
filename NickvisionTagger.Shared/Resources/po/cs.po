#~ "            - filename\n"
#~ "            - title\n"
#~ "            - artist\n"
#~ "            - album\n"
#~ "            - year\n"
#~ "            - track\n"
#~ "            - albumartist\n"
#~ "            - genre\n"
#~ "            - comment\n"
#~ "            - bpm\n"
#~ "            - composer\n"
#~ "            - description\n"
#~ "            - publisher\n"
#~ "            - isrc\n"
