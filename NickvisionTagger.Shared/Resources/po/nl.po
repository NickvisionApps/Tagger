#~ "            Each property is separated by a semicolon. Notice how the "
#~ "last property does not end in a semicolon.\n"
#~ "\n"
#~ "            [Valid Properties]\n"
#~ "            - filename\n"
#~ "            - title\n"
#~ "            - artist\n"
#~ "            - album\n"
#~ "            - year\n"
#~ "            - track\n"
#~ "            - albumartist\n"
#~ "            - genre\n"
#~ "            - comment\n"
#~ "            - bpm\n"
#~ "            - composer\n"
#~ "            - description\n"
#~ "            - publisher\n"
#~ "            - isrc\n"
#~ "\n"
#~ "            [Syntax Checking]\n"
